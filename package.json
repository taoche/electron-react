{
  "name": "electron-react",
  "version": "0.0.1",
  "description": "a electron boilerplate and use react",
  "author": "taoche",
  "license": "MIT",
  "private": true,
  "main": "./app/dist/main.js",
  "scripts": {
    "prettier": "prettier --write 'src/**/*{.js,.tsx,ts}'",
    "build": "node build/build.js && build --config $npm_package_config_builder",
    "build:dir": "node build/build.js && electron-builder --dir",
    "build:clean": "cross-env BUILD_TARGET=clean node build/build.js",
    "dev": "node build/dev-runner.js",
    "pack": "npm run pack:main && npm run pack:renderer",
    "pack:main": "webpack --progress --colors --config build/webpack.main.config.js",
    "pack:renderer": "webpack --progress --colors --config build/webpack.renderer.config.js",
    "postinstall": "electron-builder install-app-deps"
  },
  "config": {
    "builder": "./build/builder.alpha.json"
  },
  "devDependencies": {
    "@babel/core": "7.0.0-beta.49",
    "@babel/plugin-syntax-decorators": "7.0.0-beta.49",
    "@babel/plugin-syntax-jsx": "7.0.0-beta.49",
    "@babel/plugin-syntax-typescript": "7.0.0-beta.49",
    "@babel/polyfill": "7.0.0-beta.49",
    "@babel/preset-env": "7.0.0-beta.49",
    "@types/node": "10.3.3",
    "@types/react": "16.3.17",
    "@types/react-dom": "16.0.6",
    "@types/react-router": "4.0.26",
    "@types/react-router-dom": "4.2.7",
    "awesome-typescript-loader": "5.2.0",
    "babel-loader": "8.0.0-beta.3",
    "cfonts": "^2.1.3",
    "chalk": "^2.4.1",
    "compression-webpack-plugin": "^1.1.11",
    "copy-webpack-plugin": "^4.5.2",
    "cross-env": "^5.2.0",
    "css-loader": "1.0.0",
    "cssnano": "4.0.1",
    "devtron": "^1.4.0",
    "dotenv-webpack": "1.5.7",
    "electron": "^2.0.4",
    "electron-builder": "^20.19.2",
    "electron-debug": "^2.0.0",
    "electron-devtools-installer": "^2.2.4",
    "file-loader": "1.1.11",
    "friendly-errors-webpack-plugin": "1.7.0",
    "html-webpack-plugin": "3.2.0",
    "mini-css-extract-plugin": "0.4.1",
    "multispinner": "^0.2.1",
    "node-notifier": "^5.2.1",
    "node-sass": "4.9.2",
    "optimize-css-assets-webpack-plugin": "^4.0.3",
    "ora": "^2.1.0",
    "postcss-import": "11.1.0",
    "postcss-load-config": "2.0.0",
    "postcss-loader": "2.1.6",
    "postcss-preset-env": "5.2.1",
    "prettier": "^1.13.7",
    "react-hot-loader": "4.3.3",
    "rimraf": "^2.6.2",
    "sass-loader": "7.0.3",
    "semver": "^5.5.0",
    "shelljs": "^0.8.2",
    "style-loader": "0.21.0",
    "tsconfig-paths-webpack-plugin": "3.2.0",
    "tslint": "5.10.0",
    "tslint-config-prettier": "^1.13.0",
    "tslint-eslint-rules": "^5.3.1",
    "tslint-loader": "3.6.0",
    "tslint-react": "^3.6.0",
    "typescript": "2.9.2",
    "uglifyjs-webpack-plugin": "1.2.7",
    "url-loader": "^1.0.1",
    "webpack": "4.16.0",
    "webpack-bundle-analyzer": "^2.13.1",
    "webpack-cli": "3.0.8",
    "webpack-dev-server": "3.1.4",
    "webpack-hot-middleware": "^2.22.2",
    "webpack-merge": "^4.1.3"
  },
  "dependencies": {
    "axios": "^0.18.0",
    "electron-config": "^1.0.0",
    "electron-is": "^2.4.1",
    "electron-log": "^2.2.16",
    "emotion": "^9.2.5",
    "history": "4.7.2",
    "mobx": "^5.0.3",
    "mobx-react": "^5.2.3",
    "react": "16.4.1",
    "react-dom": "16.4.1",
    "react-router": "4.3.1",
    "react-router-dom": "4.3.1"
  },
  "engines": {
    "node": ">= 8.0.0",
    "npm": ">= 5.0.0"
  },
  "browserslist": [
    "> 1%",
    "last 1 versions",
    "not ie <= 8"
  ]
}
