{
  "presets": [
    ["@babel/preset-env", {
      "modules": false,
      "targets": {
        "browsers": ["last 1 versions"]
      }
    }]
  ],
  "plugins": [
    "@babel/plugin-syntax-typescript",
    "@babel/plugin-syntax-decorators",
    "@babel/plugin-syntax-jsx",
    "react-hot-loader/babel"
  ]
}
